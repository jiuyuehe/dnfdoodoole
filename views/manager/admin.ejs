<%- include ../header.ejs %>
<body >
<div class="container">
    <div  class=" col-lg-4 well ">
        <div class="well-lg"> <h3>doodoole 后台管理员登录</h3></div>
        <form class="form-horizontal" role="form">
            <div class="form-group">
                <label for="email_back" class="col-lg-3 control-label">邮箱：</label>
                <div class="col-lg-10">
                    <input type="email" class="form-control" id="email_back" placeholder="请输入邮箱">
                </div>
            </div>
            <div class="form-group">
                <label for="pwd_back" class="col-lg-3 control-label">密码：</label>
                <div class="col-lg-10">
                    <input type="password" class="form-control" id="pwd_back" placeholder="请输入密码">
                </div>
            </div>
            <div class="form-group">
                <div class="col-lg-offset-2 col-lg-10">
                    <div class="checkbox">
                        <label>
                            <input type="checkbox"> 记住密码
                        </label>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="col-lg-offset-2 col-lg-10">
                    <button type="submit" class="btn btn-primary" onclick="backLogin()">登录</button>
                </div>
            </div>
        </form>
    </div>
</div>
</body>

<%- include ../footer.ejs %>